<?xml version="1.0" encoding="UTF-8"?>
<!-- Could not determine the version of your GWT SDK; using the module DTD from GWT 1.6.4. You may want to change this. -->
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.6.4//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.6.4/distro-source/core/src/gwt-module.dtd">

<module rename-to='main'>

	<!-- Inherit the core Web Toolkit stuff.                        -->
	<inherits name="com.google.gwt.user.User"/>
	<inherits name="com.google.gwt.user.theme.standard.Standard"/>
	<!-- for GWT2.3 <inherits name="com.google.web.bindery.event.Event"/> -->	

	<!-- Other modules inherits										-->
	<!-- ======================										-->
	
	<!-- MVP	                                      				-->
	<!-- <inherits name="com.google.gwt.place.Place"/>	-->
	<!-- <inherits name="com.google.gwt.activity.Activity"/>	-->

	<!-- Inherit GWT Mobile with phonegap -->
		<!-- Inherit GWT Mobile UI module -->
	<inherits name='com.gwtmobile.ui.gwtmobile_ui'/>
		<!-- Gwt-Phonegap module inherits                  			-->
	<inherits name='com.gwtmobile.phonegap.gwtmobile_phonegap' />
		<!-- Gwt Persistence module inherits                  		-->
	<inherits name='com.gwtmobile.persistence.gwtmobile_persistence' />
	<set-property name="user.agent" value="safari"/>

	<!-- Inherit Slim3 GWT -->
	<inherits name="org.slim3.gwt.emul.S3Emulation" />
	
	<!-- Specify the paths for translatable code                    -->
  	<source path='client'/>
  	<source path='shared'/>
  	
	<!-- Specify the app entry point class.                         -->
	<entry-point class="com.gwtmobile.persistence.client.Main"/>
	
<!--  may remove if client have replacement -->

	<!-- Self-created modules inherits                                      -->
	
</module>